#!/usr/bin/env bash
# is a factors challenge

import sys

def factors(n):
    """Returns a list of the factors of n"""
    return [i for i in range(2, n//2 + 1) if n % i == 0]

def main():
    # Read the file name from the command line
    filename = sys.argv[1]

    # Open the file and read the numbers
    with open(filename, 'r') as f:
        numbers = [int(line.strip()) for line in f]

    # Factorize each number and print the result
    for n in numbers:
        p, q = (n, 1)
        for i in range(2, n//2 + 1):
            if n % i == 0:
                p, q = i, n//i
                break
        print(f"{n} = {p} * {q}")

if __name__ == '__main__':
    main()
